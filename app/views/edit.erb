<form action="/articles/<%= @article.id %>"method="POST">
<input id="hidden" type="hidden" name="_method" value= "patch">
  <h2> Title: <input id="title" type="text" name="title" value="<%= @article.title %>"/></h2>
  <h2> Content: <input id="content" type="text" name="content" value="<%= @article.content%>"/></h2>
  <input type="submit" value="Submit" />
</form>